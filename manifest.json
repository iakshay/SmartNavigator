{
  "name": "Say That!",
  "version": "0.1",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["js/detect.js"],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "manifest_version": 2
}
